        <folder name="MIMEResolver">
            <file name="ZsResolver.xml" url="ZsResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.sf.rem.loaders.Bundle"/>
            </file>
            <file name="ZulResolver.xml" url="ZulResolver.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.sf.rem.loaders.Bundle"/>
            </file>
        </folder>
    </folder>
    <folder name="Templates">
        <folder name="JSP_Servlet">
            <file name="ZulTemplate.zul" url="ZulTemplate.zul">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.sf.rem.loaders.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="web-types"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/net/sf/rem/resources/ZUL.html"/>
                <attr name="templateWizardIterator" methodvalue="net.sf.rem.loaders.wizards.ZFileIterator.createZulIterator"/>     
            </file>
            <file name="ZsTemplate.zs" url="ZsTemplate.zs">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="net.sf.rem.loaders.Bundle"/>
                <attr name="template" boolvalue="true"/>
                <attr name="templateCategory" stringvalue="web-types"/>
                <attr name="templateWizardURL" urlvalue="nbresloc:/net/sf/rem/resources/ZS.html"/>
                <attr name="templateWizardIterator" methodvalue="net.sf.rem.loaders.wizards.ZFileIterator.createZsIterator"/>
            </file>
        </folder>
    </folder>
    
    <folder name="Plugins">
        <!-- Register XML module catalog plugin -->
        <folder name="XML">
            <folder name="UserCatalogs">
                <file name="net-sf-rem-loaders-ZulCatalog.instance">
                    <attr name="instanceCreate" newvalue="net.sf.rem.loaders.ZulCatalog"/>
                    <attr name="instanceOf" stringvalue="org.netbeans.modules.xml.catalog.spi.CatalogReader"/>
                </file>
            </folder>
            <folder name="GrammarQueryManagers">
                <!-- register semantics grammar for zul file due to code completion -->
                <file name="net-sf-rem-loaders-ZulGrammarQueryManager.instance"/>
                <attr name="net-sf-rem-loaders-ZulGrammarQueryManager.instance/semantics-grammar-to-generic-grammar-separator" boolvalue="true"/>
            </folder> <!-- GrammarQueryManagers -->
        </folder>
    </folder>
    
